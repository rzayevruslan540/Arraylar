#include <iostream>
#include <cmath>
using namespace std;

void calculate(int a[], int n){
    double sum = 0;
    for(int i=0;i<10;i++) sum+=a[i];
    double avg = sum/10;

    double product = 1;
    for(int i=10;i<20;i++) product*=a[i];
    double geo = pow(product, 1.0/10);

    cout << "Ilk yarinin ededi ortasi: " << avg << endl;
    cout << "Ikinci yarinin hendesi ortasi: " << geo << endl;
}

int main(){
    int a[20];
    for(int i=0;i<20;i++) cin >> a[i];
    calculate(a,20);
}
