#include <iostream>
#include <cmath>
using namespace std;

void calculate(int a[], int n){
    int max=a[0], min=a[0];
    double sum=0, product=1;

    for(int i=0;i<n;i++){
        if(a[i] > max) max=a[i];
        if(a[i] < min) min=a[i];
        sum+=a[i];
        product*=a[i];
    }

    double avg = sum/n;
    double geo = pow(product, 1.0/n);

    cout<<"Max: "<<max<<endl;
    cout<<"Min: "<<min<<endl;
    cout<<"Ededi orta: "<<avg<<endl;
    cout<<"Hendesi orta: "<<geo<<endl;
}

int main(){
    int a[25];
    for(int i=0;i<25;i++) cin>>a[i];
    calculate(a,25);
}
